					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{base_url}}/sentences",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"sentences"
							]
						},
						"description": "Lấy danh sách tất cả sentences từ row words"
